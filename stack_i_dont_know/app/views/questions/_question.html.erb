<tr id="<%= question.id %>">
  <td>
    <span class="votes"><%= question.votes %></span>
    <%= link_to "Up", upvote_question_path(question), method: :patch, remote: true %>
    <%= link_to "Down", downvote_question_path(question), method: :patch, remote: true %>
  </td>
  <td> <%= link_to question.title, question_path(question.id) %> </td>
</tr>
