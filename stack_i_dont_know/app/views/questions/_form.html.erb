<h2>New Question</h2>

<%= form_for(@question, remote: true) do |f| %>
  <%= render 'error' %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </p>

  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>

