<div>
  <%= link_to 'Create New Question', new_question_path %>

  <table>
    <th>Votes</th>
    <th>Title</th>
  <% @questions.each do |question| %>
    <tr>
      <td> <%= question.votes %> </td>
      <td> <%= link_to question.title, question_path(question.id) %> </td>
    </tr>
  <% end %>
  </table>
</div>
